{% extends 'base.html.twig' %}

{% block title %}User{% endblock %}

{% block body %}
    <h1>Modifier l'utilisateur : {{ user.username }} </h1>

    {{ form_start(requestForm) }}
    {{ form_row(requestForm.username) }}
    {{ form_row(requestForm.nom) }}
    {{ form_row(requestForm.prenom) }}
    {{ form_row(requestForm.telephone) }}
    {{ form_row(requestForm.mail) }}
    {{ form_row(requestForm.campus) }}
    {{ form_row(requestForm.imageFile) }}




    <button type="submit" class="btn">Modifier</button>
    {{ form_end(requestForm) }}


{% endblock %}
